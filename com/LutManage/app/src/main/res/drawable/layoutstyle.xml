<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>


                <solid android:color="#FFFFFF" />

                <stroke
                    android:width="0.03dp"
                    android:color="#336633"/>

                <padding
                    android:bottom="1dp"
                    android:left="1dp"
                    android:right="1dp"
                    android:top="1dp" />

                <corners android:radius="10dp" />
            </shape>
        </item>
    <item android:state_pressed="true">
        <shape>


            <solid android:color="#FFFFFF" />

            <stroke
                android:width="0.03dp"
                android:color="#CC3333"/>

            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />

            <corners android:radius="10dp" />
        </shape>
    </item>
</selector>
